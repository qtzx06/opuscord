# your bot account's user id (right click your profile -> copy id)
BOT_USER_ID=123456789012345678

# anthropic api key
ANTHROPIC_API_KEY=sk-ant-...

# voyage api key (for embeddings)
VOYAGE_API_KEY=pa-...
